{
  "version": 2,
  "static": {
    "includes": ["node_modules/antd/dist/reset.css"]
  },
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next",
      "command": "yarn run build",
      "output": "dist"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_API": "https://localhost:0402/"
  }
}
