{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "NEXT_API": "https://localhost:0402/"
    },
    "static": {
        "includes": [
          "node_modules/antd/dist/reset.css"
        ]
      }
      
  }
  